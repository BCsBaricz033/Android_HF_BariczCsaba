<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Fragment2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/image"

            app:layout_constraintEnd_toStartOf="@+id/TextView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="50dp"
            android:layout_height="53dp"
            tools:layout_editor_absoluteX="77dp"
            tools:layout_editor_absoluteY="52dp" />

        <TextView
            android:id="@+id/TextView"
            app:layout_constraintEnd_toStartOf="@+id/vetel"
            app:layout_constraintStart_toEndOf="@+id/image"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="107dp"
            android:layout_height="25dp"
            android:gravity="left"
            android:textColor="@color/black"
            android:textFontWeight="@integer/material_motion_duration_medium_1"
            android:textSize="20dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="64dp"
            tools:layout_editor_absoluteY="140dp" />

        <TextView
            android:id="@+id/teljes"
            app:layout_constraintEnd_toStartOf="@+id/eladas"
            app:layout_constraintStart_toEndOf="@+id/image"
            app:layout_constraintTop_toBottomOf="@+id/TextView"
            android:layout_width="107dp"
            android:layout_height="27dp"
            tools:layout_editor_absoluteX="64dp"
            tools:layout_editor_absoluteY="198dp" />

        <TextView
            android:id="@+id/veteliAr"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/eladas"
            app:layout_constraintTop_toBottomOf="@+id/eladasiAr"
            android:layout_width="102dp"
            android:layout_height="27dp"
            android:gravity="right"
            android:textColor="@color/black"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="266dp"
            tools:layout_editor_absoluteY="94dp" />

        <TextView
            android:id="@+id/eladasiAr"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/vetel"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="101dp"
            android:layout_height="25dp"
            android:layout_weight="1"
            android:gravity="right"
            android:textColor="@color/black"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="267dp"
            tools:layout_editor_absoluteY="35dp" />

        <TextView
            android:id="@+id/eladas"
            app:layout_constraintEnd_toStartOf="@+id/veteliAr"
            app:layout_constraintStart_toEndOf="@+id/teljes"
            app:layout_constraintTop_toBottomOf="@+id/vetel"
            android:layout_width="69dp"
            android:layout_height="27dp"
            android:layout_weight="1"
            android:gravity="right"
            tools:layout_editor_absoluteX="198dp"
            tools:layout_editor_absoluteY="33dp" />

        <TextView
            android:id="@+id/vetel"
            app:layout_constraintEnd_toStartOf="@+id/eladasiAr"
            app:layout_constraintStart_toEndOf="@+id/TextView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="69dp"
            android:layout_height="25dp"
            android:gravity="right"
            tools:layout_editor_absoluteX="198dp"
            tools:layout_editor_absoluteY="94dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>